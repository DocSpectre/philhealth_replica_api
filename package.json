{
  "name": "Philhealth-Replica-API",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "",
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist/index.js",
    "dev": "nodemon",
    "build": "ncc build --source-map app.ts -o dist",
    "build:tsc": "tsc",
    "build-sql-files": "tsc --project tsconfig.sql.json",
    "generate-sql": "npm run build-sql-files && node -r dotenv/config src/scripts/generate-sql dist-sql",
    "generate-schemas": "node src/scripts/generate-schemas",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "^6.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@vercel/ncc": "^0.38.3",
    "jest": "^28.1.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.65.1"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "dist",
      "**/*.test.ts"
    ],
    "ext": "js,json,ts",
    "watch": [
      "."
    ]
  }
}
